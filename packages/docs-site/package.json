{
  "name": "@chart-parts/docsite",
  "description": "Docs website for chart-parts",
  "version": "0.1.3",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/chart-parts.git"
  },
  "scripts": {
    "clean_gatsby": "gatsby clean",
    "clean_docs": "essex clean static/apidocs",
    "clean_generated": "essex clean static/apidocs static/examples",
    "clean": "run-p  clean_docs clean_generated clean_gatsby",
    "start": "gatsby develop -H 0.0.0.0",
    "start:docsite": "yarn start",
    "type-check": "tsc --noEmit",
    "build_docs": "run-s 'gen:doc:*'",
    "gen:doc": "shx echo \"building $LIB\" && typedoc --theme default --readme ../$LIB/readme.md --out static/apidocs/$LIB --tsconfig ../$LIB/tsconfig.typedoc.json --excludePrivate --hideGenerator ../$LIB/src/index.ts",
    "gen:doc:builder": "LIB=api-builder yarn gen:doc",
    "gen:doc:interfaces": "LIB=api-interfaces yarn gen:doc",
    "gen:doc:react": "LIB=api-react yarn gen:doc",
    "gen:doc:orchestrator": "LIB=processing-orchestrator yarn gen:doc",
    "gen:doc:scenegraph": "LIB=processing-scenegraph yarn gen:doc",
    "gen:doc:vsvg": "LIB=processing-vsvg yarn gen:doc",
    "gen:doc:scales": "LIB=util-scales yarn gen:doc",
    "gen:doc:shapes": "LIB=util-shapes yarn gen:doc",
    "gen:doc:transform": "LIB=util-transform yarn gen:doc",
    "gen:doc:react-native-svg": "LIB=renderer-react-native-svg yarn gen:doc",
    "gen:doc:react-svg": "LIB=renderer-react-svg yarn gen:doc",
    "copy_examples": "shx cp -r ../docs-examples/src/renderless static/examples",
    "format_examples": "prettier static/examples/**/* --use-tabs=false --write",
    "write_example_boilerplate": "node write_sample_boilerplate.js",
    "process_examples": "run-s copy_examples format_examples write_example_boilerplate",
    "build_gatsby": "gatsby build --prefix-paths",
    "clean_release": "run-s clean build release",
    "build": "run-s clean process_examples build_docs build_gatsby"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@chart-parts/examples": "workspace:packages/docs-examples",
    "@chart-parts/interfaces": "workspace:packages/api-interfaces",
    "@chart-parts/orchestrator": "workspace:packages/processing-orchestrator",
    "@chart-parts/react": "workspace:packages/api-react",
    "@chart-parts/react-svg-renderer": "workspace:packages/renderer-react-svg",
    "@chart-parts/scales": "workspace:packages/util-scales",
    "@chart-parts/testdata": "workspace:packages/docs-testdata",
    "@chart-parts/transform": "workspace:packages/util-transform",
    "babel-plugin-styled-components": "^2.0.2",
    "d3-scale": "^3.3.0",
    "debug": "^4.3.3",
    "gatsby": "^4.3.0",
    "gatsby-plugin-react-helmet": "^5.3.0",
    "gatsby-plugin-sharp": "^4.3.0",
    "gatsby-plugin-styled-components": "^5.3.0",
    "gatsby-plugin-typography": "^4.3.0",
    "gatsby-remark-autolink-headers": "^5.3.0",
    "gatsby-remark-copy-linked-files": "^5.3.0",
    "gatsby-remark-images": "^6.3.0",
    "gatsby-remark-prismjs": "^6.3.0",
    "gatsby-source-filesystem": "^4.3.0",
    "gatsby-transformer-remark": "^5.3.0",
    "gatsby-transformer-sharp": "^4.3.0",
    "gsap": "^3.8.0",
    "prismjs": "^1.27.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-tabs": "^3.2.3",
    "react-typography": "^0.16.20",
    "react-use-dimensions": "^1.2.1",
    "rehype-react": "^6.2.1",
    "styled-components": "^5.3.3",
    "typography": "^0.16.21",
    "typography-theme-github": "^0.16.19"
  },
  "devDependencies": {
    "@essex/scripts": "^18.2.1",
    "@types/d3-scale": "^2.2.6",
    "@types/debug": "^4.1.7",
    "@types/gsap": "^1.20.2",
    "@types/prettier": "^2.4.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@types/react-tabs": "^2.3.3",
    "@types/styled-components": "^5.1.17",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "regenerator-runtime": "^0.13.9",
    "shx": "^0.3.3",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.3"
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ]
}
