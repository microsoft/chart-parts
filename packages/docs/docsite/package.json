{
  "name": "@chart-parts/docsite",
  "description": "Docs website for chart-parts",
  "version": "0.1.3",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@chart-parts/examples": "^0.1.3",
    "@chart-parts/interfaces": "^0.1.3",
    "@chart-parts/orchestrator": "^0.1.3",
    "@chart-parts/react": "^0.1.3",
    "@chart-parts/react-svg-renderer": "^0.1.3",
    "@chart-parts/scales": "^0.1.3",
    "@chart-parts/testdata": "^0.1.3",
    "@chart-parts/transform": "^0.1.3",
    "@types/d3-scale": "^2.1.1",
    "@types/debug": "^4.1.4",
    "@types/gsap": "^1.20.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-tabs": "^2.3.1",
    "@types/styled-components": "^5.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "d3-scale": "^3.2.0",
    "debug": "^4.1.0",
    "gatsby": "^2.17.11",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-styled-components": "^3.1.12",
    "gatsby-plugin-typescript": "^2.1.16",
    "gatsby-plugin-typography": "^2.3.16",
    "gatsby-remark-autolink-headers": "^2.1.17",
    "gatsby-remark-copy-linked-files": "^2.1.29",
    "gatsby-remark-images": "^3.1.30",
    "gatsby-remark-prismjs": "^3.3.22",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-transformer-remark": "^2.6.33",
    "gsap": "^3.0.1",
    "prismjs": "^1.16.0",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.7.0",
    "react-tabs": "^3.0.0",
    "react-typography": "^0.16.19",
    "react-use-dimensions": "^1.2.1",
    "rehype-react": "^6.0.0",
    "styled-components": "^5.1.0",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19",
    "use-debounce": "^3.0.1"
  },
  "license": "MIT",
  "scripts": {
    "clean_gatsby": "gatsby clean",
    "clean_docs": "essex clean static/apidocs",
    "clean_generated": "essex clean static/apidocs static/examples",
    "clean": "run-p clean_generated clean_gatsby",
    "build_docs": "run-s clean_docs gen:docs",
    "start": "gatsby develop -H 0.0.0.0",
    "type-check": "tsc --noEmit",
    "gen:docs": "run-p gen:doc:*",
    "gen:doc": "cross-env-shell \"typedoc --theme default --out static/apidocs/$LIB --tsconfig ../../$LIB/tsconfig.typedoc.json  --ignoreCompilerErrors --excludePrivate --excludeNotExported --hideGenerator --mode file\"",
    "gen:doc:builder": "cross-env LIB=client/builder yarn gen:doc",
    "gen:doc:interfaces": "cross-env LIB=client/interfaces yarn gen:doc",
    "gen:doc:react": "cross-env LIB=client/react yarn gen:doc",
    "gen:doc:orchestrator": "cross-env LIB=processing/orchestrator yarn gen:doc",
    "gen:doc:scenegraph": "cross-env LIB=processing/scenegraph yarn gen:doc",
    "gen:doc:vsvg": "cross-env LIB=processing/vsvg yarn gen:doc",
    "gen:doc:scales": "cross-env LIB=util/scales yarn gen:doc",
    "gen:doc:shapes": "cross-env LIB=util/shapes yarn gen:doc",
    "gen:doc:transform": "cross-env LIB=util/transform yarn gen:doc",
    "gen:doc:react-native-svg": "cross-env LIB=renderers/react-native-svg yarn gen:doc",
    "gen:doc:react-svg": "cross-env LIB=renderers/react-svg yarn gen:doc",
    "copy_examples": "shx cp -r ../examples/src/renderless static/examples",
    "format_examples": "prettier static/examples/**/* --use-tabs=false --write",
    "write_example_boilerplate": "node write_sample_boilerplate.js",
    "process_examples": "run-s copy_examples format_examples write_example_boilerplate",
    "build_gatsby": "gatsby build --prefix-paths",
    "clean_release": "run-s clean build release",
    "build": "run-s clean process_examples build_docs build_gatsby"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.5",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
