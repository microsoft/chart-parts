{
  "name": "@chart-parts/docsite",
  "description": "Docs website for chart-parts",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@chart-parts/examples": "^0.1.2",
    "@chart-parts/interfaces": "^0.1.1",
    "@chart-parts/orchestrator": "^0.1.1",
    "@chart-parts/react": "^0.1.2",
    "@chart-parts/react-svg-renderer": "^0.1.1",
    "@chart-parts/scales": "^0.1.1",
    "@chart-parts/testdata": "^0.1.1",
    "@chart-parts/transform": "^0.1.1",
    "@types/d3-scale": "^2.1.1",
    "@types/debug": "^4.1.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.10",
    "@types/react-tabs": "^2.3.1",
    "@types/styled-components": "^4.1.19",
    "babel-plugin-styled-components": "^1.10.6",
    "d3-scale": "^3.1.0",
    "debug": "^4.1.0",
    "gatsby": "^2.15.16",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-styled-components": "^3.1.5",
    "gatsby-plugin-typescript": "^2.1.8",
    "gatsby-plugin-typography": "^2.3.7",
    "gatsby-remark-autolink-headers": "^2.1.10",
    "gatsby-remark-copy-linked-files": "^2.1.19",
    "gatsby-remark-images": "^3.1.22",
    "gatsby-remark-prismjs": "^3.3.13",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-remark": "^2.6.22",
    "prismjs": "^1.16.0",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.7.0",
    "react-tabs": "^3.0.0",
    "react-typography": "^0.16.19",
    "rehype-react": "^4.0.1",
    "styled-components": "^4.3.2",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19",
    "use-debounce": "^3.0.1"
  },
  "license": "MIT",
  "scripts": {
    "clean_gatsby": "gatsby clean",
    "clean_generated": "rimraf apidocs static/examples",
    "clean": "run-p clean_generated clean_gatsby",
    "build_docs": "run-s clean:docs gen:docs",
    "start": "gatsby develop -H 0.0.0.0",
    "type-check": "tsc --noEmit",
    "clean:docs": "rimraf static/apidocs",
    "gen:docs": "run-p gen:doc:*",
    "gen:doc": "typedoc --theme default --out static/apidocs/$LIB --tsconfig ../../$LIB/tsconfig.typedoc.json  --ignoreCompilerErrors --excludePrivate --excludeNotExported --hideGenerator --mode file",
    "gen:doc:builder": "LIB=client/builder yarn gen:doc",
    "gen:doc:interfaces": "LIB=client/interfaces yarn gen:doc",
    "gen:doc:react": "LIB=client/react yarn gen:doc",
    "gen:doc:scene": "LIB=processing/scene yarn gen:doc",
    "gen:doc:orchestrator": "LIB=processing/orchestrator yarn gen:doc",
    "gen:doc:scenegraph": "LIB=processing/scenegraph yarn gen:doc",
    "gen:doc:vsvg": "LIB=processing/vsvg yarn gen:doc",
    "gen:doc:scales": "LIB=util/scales yarn gen:doc",
    "gen:doc:shapes": "LIB=util/shapes yarn gen:doc",
    "gen:doc:transform": "LIB=util/transform yarn gen:doc",
    "gen:doc:react-native-svg": "LIB=renderers/react-native-svg yarn gen:doc",
    "gen:doc:react-svg": "LIB=renderers/react-svg yarn gen:doc",
    "copy_examples": "cp -r ../examples/src/renderless static/examples",
    "format_examples": "prettier 'static/examples/**/*' --use-tabs=false --write",
    "write_example_boilerplate": "node write_sample_boilerplate.js",
    "process_examples": "run-s copy_examples format_examples write_example_boilerplate",
    "build_static_site": "gatsby build --prefix-paths",
    "publish_gh_pages": "NODE_DEBUG=gh-pages gh-pages -d public -t",
    "clean_release": "run-s clean build release",
    "release": "run-s clean process_examples build_docs build_static_site publish_gh_pages"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
